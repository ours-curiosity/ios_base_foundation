{
  "name": "iOSBaseFoundation",
  "version": "0.5.0",
  "summary": "iOS 基础加速库.",
  "description": "你可以使用base_foundation来构建新的app. ios 10.0, swift 5",
  "homepage": "https://github.com/ours-curiosity/ios_base_foundation",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ours-curiosity": "ours.curiosity@gmail.com"
  },
  "source": {
    "git": "https://github.com/ours-curiosity/ios_base_foundation.git",
    "tag": "0.5.0"
  },
  "social_media_url": "https://ours-curiosity.github.io/",
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreGraphics"
  ],
  "source_files": "ios_base_foundation/Classes/**/*",
  "dependencies": {
    "SnapKit": [
      "= 5.0.1"
    ],
    "Toast-Swift": [
      "= 5.0.1"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "ios_base_foundation/Core/*"
    },
    {
      "name": "Extension",
      "source_files": [
        "ios_base_foundation/Core/*",
        "ios_base_foundation/Extension/**/*"
      ]
    },
    {
      "name": "UIKit",
      "source_files": [
        "ios_base_foundation/Core/*",
        "ios_base_foundation/Extension/UIKit/*"
      ]
    }
  ],
  "swift_version": "5.0"
}
